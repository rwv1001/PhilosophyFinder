ShowSearchResults();

$('#search-results').replaceWith('<%= j render "search_results/display_results", args: {:first_result_id => @first_result_id, :last_result_id => @last_result_id, :show_previous => @show_previous, :show_next => @show_next, :query_id => @query_id}%>');

$("#group_notice").empty();
$("#search_notice").empty();
$("#search_notice").show();
<% if @search_results.length >0 %>
$("#search_notice").text("Showing  <%= @first_result_id - @absolute_first +1 %> to <%= @last_result_id - @absolute_first +1  %> of <%= @search_results.length %>, Unprocessed Results: <%= @unprocessed_sentence_count %>");


<% end %>

<% if @unprocessed_sentence_count >0 %>
ProcessMore()
<% end %>

